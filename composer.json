{
  "name": "upx/teste-backend",
  "description": "What can be better?",
  "minimum-stability" : "stable",
  "autoload": {
    "psr-4": {
      "Acme\\": [
        "src/",
        "tests/"
      ]
    }
  },
  "require": {
    "php": ">=7.1",
    "silex/silex": "~2.0",
    "guzzlehttp/guzzle": "6.2.2",
    "symfony/yaml": "^4.0",
    "knplabs/console-service-provider": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "symfony/var-dumper": "^4.0",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "scripts": {
    "psr2": [
      "vendor/squizlabs/php_codesniffer/bin/phpcs --standard=PSR2 --extensions=php,inc,lib src/"
    ]
  }
}
